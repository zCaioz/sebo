    <!--
        Added the constraints for entity Venda.
    -->
    <changeSet id="20251118234318-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="venda"
                                 constraintName="fk_venda__usuario_id"
